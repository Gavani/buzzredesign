extends ../main-layout
block variables
  -
    var title = "Buzzguide - Price History Check";
  
block content
  include ../modules/header.pug
  +header('index')
  section.rankings__breadcrumbs.price-history__breadrumbs
    .container__fluid
      ul.rankings__breadcrumbs-list
        li.rankings__breadcrumbs-item
          a(href="#").rankings__breadcrumbs-link Product Rankings 
          span.rankings__breadcrumbs-devide >>
        li.rankings__breadcrumbs-item.rankings__breadcrumbs-item_active
          span.rankkings__breadcrumbs-span Start page
  section.rankings__hero.price-history__hero
    .container
      .rankings__hero-head-text
        h2 Search
      .rankings__hero-search
        label.rankings__hero-search-label
          p.label-text__mobile Start typing a product name or copy paste an Amazon URL 
          input(type="search", name="search", placeholder='Start typing a product name or copy paste an Amazon URL ')
          span.rankings__hero-search-icon
            i(class="fas fa-search")
      .price-history__hero-attention
        p *Only amazon.in URL's are supported.
        p *Newly launched products within last 7 days or products with less than 5 customer reviews may not be available
      .rankings__hero-text OR
      .rankings__hero-category-text Browse all products in a category & find price history of your desired products.
      .rankings__hero-category
        include ../modules/menu.pug
        +menu
  section.price-history__devider
  section.price-history__drops
    .container__fluid
      .price-history__drops-head
        .price-history__drops-head-text 
          p We observed some quite good price drops today on Amazon's Super famous items (Above 1000 Amazon reviews)
        .price-history__drops-head-update
          p UPDATED TODAY: 10-Nov, 2018 (07:38 AM)
      .price-history__drops-cards
        .deals__cards-item
          .ribbon__wrapp
            .ribbon
              | Featured Price Drop
            i
          .deals__cards-item-img
            .deals__cards-item-img-drop
              p 52%
              p Price Drop
            .deals__cards-item-pic
              img(src="assets/img/contents/image1.jpg", alt="Product Image")
          .deals__cards-item-left
            .deals__cards-item-info
              .deals__cards-item-info-name
                h3 Phillips Trimmer QT4011 cordless trimmer
              .deals__cards-item-info-rating
                span.text-red 4.2 
                | rating, From 
                span.text-red 13334 
                | Amazon Reviews
              .deals__cards-item-info-rs
                p Rs 344020 on Amazon
            .deals__cards-item-rs
              .deals__cards-item-rs-30
                del Rs 89 
                span 30 days back
              .deals__cards-item-rs-today
                span Rs 49 
                span Today
              .deals__cards-item-rs-drop
                span Rs.3399 (52%) Price Drop
              .deals__cards-item-rs-lowest
                p Lowest price in 30 days: Rs.567669
              .deals__cards-item-rs-highest
                p Highest price in 30 days: Rs.867669
            .deals__cards-item-price
              .deals__cards-item-price-history
                include ../modules/card-price
                +priceHistory
              .deals__cards-item-price-links
                a(href="#").deals__cards-item-price-links-amazon See it on Amazon > 
                a(href="#").deals__cards-item-price-links-alert Or set price alert to this item at your desired price >
          .deals__cards-item-bottom-price
            .deals__cards-item-bottom-price-head
              p Amazon Price History (As on: 07-Nov, 07: 59 AM)
            .deals__cards-item-bottom-price-wrap        
